import usaddress
text = "It also unlocks the ability to teleport to the nearest and happiest food coma ğŸ‘ŒğŸ˜ğŸ”¥ğŸ– @daeho_official ğŸ“Daeho - San Mateo, Milpitas, SF"

items = usaddress.parse(text)
d = {}

for item in items:
    d[item[1]] = item[0]

address = ""

order = ["AddressNumber", "StreetName", "StreetNamePostType", "PlaceName", "StateName"]

for o in order:
    if d.get(o, None) != None:
        address += d.get(o, None) + " "

print(address)


